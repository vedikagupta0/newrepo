name: CI Workflow

on:
  push:
    branches:
      - main  # or the branch you want to trigger the action on
  pull_request:
    branches:
      - main  # or the branch you want to trigger the action on

jobs:
  test:
    runs-on: ubuntu-latest

    steps:
      - name: Checkout Code
        uses: actions/checkout@v2

      - name: 23f3003724@ds.study.iitm.ac.in
        run: echo "Hello, world!"
